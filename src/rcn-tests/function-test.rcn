# This is how functions work

fn fib(n) {
    if (n <= 1) return n;

    var a = 0;
    var b = 1;
    var result = 0;

    for (var i = 2; i <= n; i = i + 1) {
        result = a + b;
        a = b;
        b = result;
    }

    return result;
}

for (var i = 0; i < 20; i = i + 1) {
    log(fib(i));
}